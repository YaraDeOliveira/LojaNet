@model LojaNet.Models.Cliente

@{
    ViewData["Title"] = "Excluir";
}

<h2>Excluir</h2>

<h4>Confirma a exclusão deste cliente? </h4>
<p>
    @Model.Nome
</p>


<hr />
<form method="post">
    <input type="hidden" name="Id" value="@Model.Id" />
    <hr />
    <a href="/Cliente/Index" class="btn btn-dark">Voltar</a>
    <input type="submit" value="Excluir" class="btn btn-danger" />
</form>







